{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Training",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train_model.py",
            "args": [
                "-cfg",
                "./runs/TEST/LeNet5TEST_runcfg.json",
                "--outdir",
                "./runs/TEST"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Testing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test_model.py",
            "args": [
                "--mstate",
                "./runs/TEST/LeNet5/LeNet5.pt",
                "--mname",
                "LeNet5",
                "--outdir",
                "./runs/TEST/LeNet5",
                "--batch-size",
                "1"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Transfer Learning",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/transfer_learning.py",
            "args": [
                "-cfg",
                "transfer_runcfg.json",
                "-short"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Fetch MNIST Data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/mnist-data.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Fetch NOT-MNIST Data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/not-mnist-data.py",
            "console": "integratedTerminal"
        }
    ]
}